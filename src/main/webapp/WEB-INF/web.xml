<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginSerlet</servlet-name>
  	<display-name>LoginSerlet</display-name>
  	<description>login work</description>
  	<servlet-class>servlets.LoginSerlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageServlet</servlet-name>
  	<display-name>ImageServlet</display-name>
  	<description>user image</description>
  	<servlet-class>servlets.ImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginSerlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
