<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginSerlet</servlet-name>
  	<display-name>LoginSerlet</display-name>
  	<description>login work</description>
  	<servlet-class>servlets.LoginSerlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageSerlet</servlet-name>
  	<display-name>ImageSerlet</display-name>
  	<description>user image</description>
  	<servlet-class>servlets.ImageSerlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description>registration work</description>
  	<servlet-class>servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageStreamServlet</servlet-name>
  	<display-name>ImageStreamServlet</display-name>
  	<description>write steam bytes to response</description>
  	<servlet-class>servlets.ImageStreamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginSerlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/registration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ImageStreamServlet</servlet-name>
  	<url-pattern>/image</url-pattern>
  </servlet-mapping>
</web-app>
